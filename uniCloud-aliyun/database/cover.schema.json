{
	"bsonType": "object",
	"required": ["openid", "url"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"openid": {
			"description": "用户的微信openid",
			"bsonType": "string"
		},
		"url": {
			"description": "封面url",
			"bsonType": "string"
		}
	}
}